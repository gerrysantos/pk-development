rest) {insert_billease_text();} else {const billease_installment_factor = localStorage.getItem(storage_key);if (billease_installment_factor) {insert_billease_text(JSON.parse(billease_installment_factor));} else {fetch('https://billease.ph/billease_installments.json').then(response => response.json()).then(data => {localStorage.setItem(storage_key, JSON.stringify(data.installment_factor));insert_billease_text(data.installment_factor);});}}}start();</script><style>.custom-div-billease img {position: relative;bottom: 8px;margin-right: 1px;}.custom-div-billease span {font-size: 16px;font-family: 'Open-sans', 'Lato';}</style></div></div></div><div class="gf_clearfix"></div>{% unless product == empty %}{% for variant in product.variants %}<input type="hidden" id="gf-hidden-variant{{variant.id}}" value="{{variant.id}}" data-compare-price="{{ variant.compare_at_price | money | strip_html }}" data-price="{{ variant.price | money | strip_html }}">{% endfor %}<script type="text/plain" class="product-json" id="product-json{{product.id}}">{{ product | json }}</script>{% endunless %}{% endform %}{% endunless %}</div>{% assign product = currentProduct %}</div></div></div></div></div><!--gfsplit--><div data-label="Row" id="r-1545024961520" class="gf_row gf_row-gap-0 gf_row-fluid gf_row-no-padding" data-icon="gpicon-row" data-id="1545024961520" data-extraclass="" data-row-gap="0px" style="display: block;"><div class="gf_col-md-12 gf_column" id="c-1545024961546" data-id="1545024961546" style="min-height: auto;"><div data-label="Row" id="r-1547110287675" class="gf_row" data-icon="gpicon-row" data-id="1547110287675" data-extraclass=""><div class="gf_column gf_col-lg-12 gf_col-md-12 gf_col-sm-12 gf_col-xs-12" id="c-1546911520067" data-id="1546911520067" style="min-height: auto;"><div data-label="Row" id="r-1547110287774" class="gf_row gf_row-fluid gf_row-no-padding gf_row-gap-48" data-icon="gpicon-row" data-id="1547110287774" data-extraclass="" style="visibility: visible;" data-row-gap=